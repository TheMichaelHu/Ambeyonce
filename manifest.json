{
  "manifest_version": 2,

  "name": "Ambeyonce",
  "description": "A chrome extension that responsively adjusts the background music it plays in response to your mood",
  "version": "1.0",

  "background": {
      "page": "background.html",
      "scripts": ["background.js", "jquery-1.12.0.min.js"]
   },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "videoCapture"
  ]
}
